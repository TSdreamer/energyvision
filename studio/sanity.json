{
  "root": true,
  "project": {
    "name": "Equinor"
  },
  "__experimental_spaces": [
    {
      "name": "production",
      "title": "Original content",
      "default": true,
      "api": {
        "projectId": "h61q9gi9",
        "dataset": "production"
      }
    },
    {
      "name": "development",
      "title": "Test with no history (development)",
      "api": {
        "projectId": "h61q9gi9",
        "dataset": "development"
      }
    },
    {
      "name": "production_global",
      "title": "Global production, empty",
      "api": {
        "projectId": "h61q9gi9",
        "dataset": "production_global"
      }
    },
    {
      "name": "production-export",
      "title": "Export from production",
      "api": {
        "projectId": "h61q9gi9",
        "dataset": "production-export"
      }
    }
  ],
  "api": {
    "projectId": "h61q9gi9",
    "dataset": "production"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "color-list",
    "intl-input",
    "asset-source-brandmaster",
    "media",
    "@sanity/production-preview"
  ],
  "env": {
    "development": {
      "plugins": ["@sanity/vision"]
    }
  },
  "parts": [
    {
      "implements": "part:@sanity/form-builder/input/file/asset-sources",
      "path": "./parts/assetSources.js"
    },
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "name": "part:@sanity/desk-tool/structure",
      "path": "./deskStructure.js"
    },
    {
      "implements": "part:@sanity/form-builder/input/array/functions",
      "path": "./components/CustomArrayFunctions.tsx"
    },
    {
      "name": "part:@sanity/base/initial-value-templates",
      "path": "./initialValueTemplates.js"
    },
    {
      "implements": "part:@sanity/production-preview/resolve-production-url",
      "path": "./resolveProductionUrl.js"
    }
  ]
}
